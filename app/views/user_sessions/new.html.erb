<%#= authlogic_facebook_login_button %>
<%#= RPXNow.popup_code("Login using FaceBook/OpenID Account", RPX_REALM, rpx_login_url, :language=> 'en') %>


 <div class="innerContent">
		<div id="xxx">
		 <div class="progressMenu">
          <ul class="rtabs">
            <li class="alogin"> <a href="#rtab1"> </a></li>
          </ul>
         </div>
                
        <div id="rtab1" class="tab_content">
			<div class="note"><p></p>
			</div>
        	<div id="loginDWrap">
            	<table align="center">
                  <% form_for @user_session, :url => user_session_path do |f| %>
                  <%= f.error_messages %>
                  <tr>
                    <td class="formLabel">Login</td>
                    <td><%= f.text_field :login, :class => 'bigTextArea' %></td>
                  </tr>
                  <tr>
                    <td class="formLabel">Password</td>
                    <td><%= f.password_field :password, :class => 'bigTextArea' %></td>
                  </tr>
                  <tr>
                    <td>&nbsp;</td>
                    <td align="center" style="padding-top:10px;"><%= f.submit "Login" %></td>
                  </tr>
                  <% end %>
                </table>

            </div>
		</div>
</div>
